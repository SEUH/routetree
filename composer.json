{
    "name": "webflorist/routetree",
    "description": "RouteTree: Advanced Route Management for Laravel (v5.5+ and v6)",
    "keywords": ["laravel", "php", "route", "localization", "multi-language"],
    "homepage": "https://github.com/webflorist/routetree",
    "license": "MIT",
    "authors": [
        {
            "name": "Gerald Buttinger",
            "email": "gerald@code.florist"
        }
    ],
    "require": {
        "laravel/framework": ">=5.5 <7.0.0"
    },
    "require-dev": {
        "orchestra/testbench": ">=3.5"
    },
    "autoload": {
        "psr-4": {
            "Webflorist\\RouteTree\\": "src/RouteTree/",
            "RouteTreeTests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webflorist\\RouteTree\\RouteTreeServiceProvider"
            ],
            "aliases": {
                "RouteTree": "Webflorist\\RouteTree\\Facades\\RouteTree"
            }
        }
    }
}
